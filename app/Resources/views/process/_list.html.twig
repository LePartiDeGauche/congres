<h3>Processus d'amendement</h3>
{% if processes is empty %}
    <p>Pas de contenu</p>
{% else %}
    <ul>
    {% for process in processes %}
        {% if is_granted('view', process) %}
            <li>
                <a href="{{ path('process_amendment_create', {id: process.id}) }}">
                    {{ process.name }}
                </a>
            </li>
        {% endif %}
    {% endfor %}
    </ul>
{% endif %}
